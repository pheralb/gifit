{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [{
      "source": "/g",
      "destination": "https://github.com/pheralb/gifit"
    },
    {
      "source": "/github",
      "destination": "https://github.com/pheralb/notart"
    },
    {
      "source": "/tw",
      "destination": "https://twitter.com/pheralb_"
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/pheralb_"
    }
  ],
  "routes": [{
      "src": "(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "(.*).json",
      "dest": "/$1.json"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}